[user]
	name = Aryan Kushwaha
	email = 59092136+funinkina@users.noreply.github.com
	signingkey = DE21A4B61C1B09D447857E154827892C53B72CC5

[core]
	editor = nano
	excludesfile = /home/funinkina/.gitignore_global
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[credential]
	helper = /usr/bin/git-credential-manager
	credentialStore = secretservice
[credential "https://dev.azure.com"]
	useHttpPath = true
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[gpg]
	format = ssh
[gpg "ssh"]
	allowedSignersFile = /home/funinkina/.ssh/allowed_signers
[commit]
	gpgSign = true
[tag]
	gpgsign = true
